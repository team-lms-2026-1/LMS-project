{
  "intro": {
    "eyebrow": "Competency Guide",
    "title": "What is 6CS?",
    "lead": {
      "line1": "6CS is a common framework for helping learners build balanced core competencies required in future society.",
      "line2": "Beyond acquiring knowledge, it develops the ability to solve real problems, collaborate, and act responsibly."
    },
    "sections": {
      "sixCs": "The Six Competencies of 6CS",
      "learningUse": "How It Is Used in Learning",
      "formula": "Scoring Formula in Diagnosis"
    },
    "cards": {
      "creativity": {
        "title": "Creativity (Creativity)",
        "description": "The ability to view problems from new perspectives and generate and execute original ideas."
      },
      "criticalThinking": {
        "title": "Critical Thinking (Critical Thinking)",
        "description": "The ability to evaluate validity of information, judge based on evidence, and reach rational conclusions."
      },
      "collaboration": {
        "title": "Collaboration (Collaboration)",
        "description": "The ability to coordinate roles and produce outcomes together for a shared goal."
      },
      "communication": {
        "title": "Communication (Communication)",
        "description": "The ability to convey thoughts clearly and build mutual understanding through diverse methods."
      },
      "citizenship": {
        "title": "Citizenship (Citizenship)",
        "description": "An attitude of understanding social norms and responsibilities, and practicing ethical and sustainable choices."
      },
      "character": {
        "title": "Character/Self-Management (Character)",
        "description": "The strength to keep growing through sincerity, empathy, and self-directedness."
      }
    },
    "learning": {
      "line1": "6CS connects curricular classes, extracurricular activities, and project experiences with one common language. This helps learners identify strengths and areas for improvement more clearly.",
      "line2": "By reflecting on which competencies improved after activities, learners can set more concrete next goals and learning plans."
    },
    "formula": {
      "line1": "(Diagnosis Likert score * each 6CS weight) + (Diagnosis short-answer score + each 6CS weight) = Diagnosis score",
      "line2": "(Extracurricular points * assigned 6CS weight for extracurricular * 10) = Extracurricular score",
      "line3": "(Curricular points * assigned 6CS weight for curricular) = Curricular score",
      "line4": "(Diagnosis score + Extracurricular score + Curricular score) = 6CS total score",
      "line5": "Using total scores, median/average values are calculated to produce each 6CS score."
    }
  },
